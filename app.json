{
  "expo": {
    "name": "Birdiy",
    "slug": "birdiy",
    "scheme": "birdiy",
    "privacy": "unlisted",
    "sdkVersion": "34.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/icon.png",
    "splash": {
      "image": "./src/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#003991"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.birdiy.app"
    },
    "android": {
      "package": "com.birdiy.app",
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "https",
            "host": "birdiy.com"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "description": "DIY sharing",
    "facebookAppId": "595828547560598",
    "facebookDisplayName": "Birdiy",
  }
}
